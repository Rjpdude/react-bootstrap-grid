{
  "name": "twbs-react-grid",
  "version": "1.2.5",
  "version_short": "1.2",
  "description": "Twitter Bootstrap 4 Standalone Flexbox Grid System for React",
  "keywords": [
    "react-bootstrap-grid",
    "react",
    "component",
    "components",
    "react-component",
    "bootstrap",
    "flexbox",
    "responsive",
    "front-end",
    "framework",
    "ui"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rjpdude/twbs-react-grid.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Ryan J. Pridgen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rjpdude/twbs-react-grid/issues"
  },
  "homepage": "https://github.com/Rjpdude/twbs-react-grid#readme",
  "devDependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "react-dom": "^16.8.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "react": "^16.8.4"
  }
}
